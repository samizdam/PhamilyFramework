{
    "name": "phamily/framework",
    "description": "Framework for developing genealogic's tree",
    "require": {
        "zendframework/zend-db": "2.6.*",
        "robmorgan/phinx": "dev-dev"
    },
    "repositories": [
    	{
    		"type": "vcs",
    		"url": "git@github.com:samizdam/phinx.git"
    	}
    ],
    "require-dev": {
        "phpunit/phpunit": "5",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "samizdam",
            "email": "samizdam@inbox.ru"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
    	"psr-4": { 
    		"Phamily\\Framework\\" : "src/" 
    	}
    },
    "autoload-dev": {
    	"psr-4": { 
    		"Phamily\\Framework\\": "tests/", 
    		"Phamily\\tests\\" : "tests/" 
    	}
    }
    
}
